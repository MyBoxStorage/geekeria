{
  "name": "bravos-brasil-backend",
  "version": "1.0.0",
  "description": "Backend API for BRAVOS BRASIL e-commerce",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "prisma:generate": "npx prisma@5.22.0 generate --schema=../prisma/schema.prisma",
    "prisma:migrate": "npx prisma@5.22.0 migrate dev --schema=../prisma/schema.prisma",
    "prisma:status": "npx prisma@5.22.0 migrate status --schema=../prisma/schema.prisma",
    "prisma:studio": "npx prisma@5.22.0 studio --schema=../prisma/schema.prisma",
    "prisma:seed": "tsx ../prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "prisma": "^5.22.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "react-router-dom": "^6.30.3",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^24.10.1",
    "tsx": "^4.19.2",
    "typescript": "^5.9.3"
  }
}
