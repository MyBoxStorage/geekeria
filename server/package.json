{
  "name": "bravos-brasil-backend",
  "version": "1.0.0",
  "description": "Backend API for BRAVOS BRASIL e-commerce",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "prisma:generate": "npx prisma@5.22.0 generate --schema=../prisma/schema.prisma",
    "prisma:migrate": "npx prisma@5.22.0 migrate dev --schema=../prisma/schema.prisma",
    "prisma:status": "npx prisma@5.22.0 migrate status --schema=../prisma/schema.prisma",
    "prisma:studio": "npx prisma@5.22.0 studio --schema=../prisma/schema.prisma",
    "prisma:seed": "tsx ../prisma/seed.ts"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.19.0",
    "@google/generative-ai": "^0.21.0",
    "@prisma/client": "^5.22.0",
    "@supabase/supabase-js": "^2.95.3",
    "@types/multer": "^2.0.0",
    "@types/nodemailer": "^7.0.10",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "node-telegram-bot-api": "^0.67.0",
    "nodemailer": "^8.0.1",
    "prisma": "^5.22.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^24.10.1",
    "@types/node-telegram-bot-api": "^0.64.13",
    "tsx": "^4.19.2",
    "typescript": "^5.9.3"
  }
}
